{
    "manifest_version": 2,
    "name": "YouTube Bypass",
    "version": "1.0",
    "author": "TheTank20",
    "description": "Bypasses YouTube's ad block ban.", 
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png",
        "256": "icons/256.png"
    },
    "permissions": [
        "activeTab",
        "webRequest",
        "webRequestBlocking",
        "*://*.youtube.com/*"
    ],
    "background": {
        "scripts": ["background.js"]
    }
  }
  